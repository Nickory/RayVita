    <!-- rPPG FAQ -->
    <string name="faq_rppg_what_is">rPPG 是什么？</string>
    <string name="faq_rppg_what_is_answer">远程光电容积描记技术，基于摄像头提取面部颜色变化分析生理数据。</string>

    <string name="faq_rppg_contact">rPPG 测量需要接触皮肤吗？</string>
    <string name="faq_rppg_contact_answer">不需要，是完全非接触的视觉测量方法。</string>

    <string name="faq_rppg_equipment">测量时需要什么设备？</string>
    <string name="faq_rppg_equipment_answer">前置摄像头、正常光照即可，无需额外硬件。</string>

    <string name="faq_rppg_metrics">支持测量哪些指标？</string>
    <string name="faq_rppg_metrics_answer">当前支持心率、呼吸频率、HRV（心率变异性）。</string>

    <string name="faq_rppg_future">未来会扩展哪些功能？</string>
    <string name="faq_rppg_future_answer">房颤检测、情绪识别、呼吸分析等功能已规划。</string>

    <string name="faq_rppg_duration">测量建议持续多久？</string>
    <string name="faq_rppg_duration_answer">建议至少 20 秒，以获得稳定数据。</string>

    <string name="faq_rppg_tips">测量过程中需要注意什么？</string>
    <string name="faq_rppg_tips_answer">保持安静、面部居中、避免剧烈移动与过暗环境。</string>

    <string name="faq_rppg_failure">为什么测量失败？</string>
    <string name="faq_rppg_failure_answer">光线不足、遮挡、面部不完整等会影响推理质量。</string>

    <string name="faq_rppg_multitask">测量过程中可以做其他操作吗？</string>
    <string name="faq_rppg_multitask_answer">建议专注测量，避免切换 App 或离开画面。</string>

    <string name="faq_rppg_accuracy">测量数据准确吗？</string>
    <string name="faq_rppg_accuracy_answer">rPPG 属于估算型方法，数据仅供参考，非医疗诊断。</string>

    <!-- Data FAQ -->
    <string name="faq_data_auto_save">测量数据是否自动保存？</string>
    <string name="faq_data_auto_save_answer">是的，测量完成后将自动记录在本地历史中。</string>

    <string name="faq_data_history">历史记录在哪查看？</string>
    <string name="faq_data_history_answer">进入 Insight → History 页面查看详细图表与趋势。</string>

    <string name="faq_data_clear">如何清除历史数据？</string>
    <string name="faq_data_clear_answer">设置页面提供清除本地数据选项。</string>

    <string name="faq_data_export">数据是否支持导出？</string>
    <string name="faq_data_export_answer">支持，可导出 CSV/JSON 格式供分析使用。</string>

    <string name="faq_data_trends">支持趋势查看吗？</string>
    <string name="faq_data_trends_answer">支持按天、周、月查看心率、呼吸频率变化。</string>

    <!-- Cloud FAQ -->
    <string name="faq_cloud_enable">如何开启云同步？</string>
    <string name="faq_cloud_enable_answer">登录后在 Insight → History 页右上角点击开启。</string>

    <string name="faq_cloud_realtime">数据是否实时同步？</string>
    <string name="faq_cloud_realtime_answer">需手动触发同步或在测量完成后自动上传。</string>

    <string name="faq_cloud_device_change">换设备数据会丢失吗？</string>
    <string name="faq_cloud_device_change_answer">开启云同步后，可在任意设备恢复历史数据。</string>

    <string name="faq_cloud_sync_fail">同步失败怎么办？</string>
    <string name="faq_cloud_sync_fail_answer">检查网络连接，稍后重试，或联系技术支持。</string>

    <string name="faq_cloud_security">我的数据安全吗？</string>
    <string name="faq_cloud_security_answer">数据传输与存储采用加密，遵循隐私政策。</string>

    <!-- AI FAQ -->
    <string name="faq_ai_theme">如何生成个性化主题？</string>
    <string name="faq_ai_theme_answer">在"主题切换"页面输入一句话，自动生成 AI 主题。</string>

    <string name="faq_ai_suggestions">AI 建议卡从哪里来？</string>
    <string name="faq_ai_suggestions_answer">每日从 Insight 模块中自动提取精选健康建议。</string>

    <string name="faq_ai_network">AI 功能需要联网吗？</string>
    <string name="faq_ai_network_answer">是的，需连接 DeepSeek API 进行推理。</string>

    <string name="faq_ai_edit">可以编辑 AI 推荐的内容吗？</string>
    <string name="faq_ai_edit_answer">目前为系统生成，未来考虑支持收藏与反馈。</string>

    <string name="faq_ai_theme_basis">主题生成依据是什么？</string>
    <string name="faq_ai_theme_basis_answer">基于文本理解与调色原理输出和谐主题方案。</string>

    <!-- Web FAQ -->
    <string name="faq_web_synapse">RayVita-Synapse 是什么？</string>
    <string name="faq_web_synapse_answer">RayVita 网页端平台，用于查看和导出健康报告。</string>

    <string name="faq_web_features">网页版支持哪些功能？</string>
    <string name="faq_web_features_answer">健康趋势图、测量记录、PDF/HTML 报告生成等。</string>

    <string name="faq_web_usage">如何使用网页端？</string>
    <string name="faq_web_usage_answer">在 App 中开启云同步，登录网页端查看完整数据。</string>

    <string name="faq_web_share">网页报告是否可分享？</string>
    <string name="faq_web_share_answer">支持导出 HTML 报告，可通过链接或邮箱分享。</string>

    <string name="faq_web_security">网页是否安全？</string>
    <string name="faq_web_security_answer">基于 HTTPS 与账户权限验证，保障隐私安全。</string>

